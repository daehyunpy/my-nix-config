{
  "agent": {
    "default_model": {
      "model": "gemini-3-pro-preview",
      "provider": "google",
    },
  },
  "autosave": "on_focus_change",
  "buffer_font_size": 12.0,
  "edit_predictions": {
    "provider": "zed",
  },
  "format_on_save": "on",
  "git_panel": {
    "tree_view": true,
  },
  "helix_mode": true,
  "inlay_hints": {
    "enabled": true,
  },
  "languages": {
    "Python": {
      "code_actions_on_format": {
        "source.fixAll.ruff": true,
        "source.organizeImports.ruff": true,
      },
    },
    "SQL": {
      "tab_size": 2,
    },
  },
  "relative_line_numbers": "enabled",
  "telemetry": {
    "diagnostics": false,
    "metrics": false,
  },
  "terminal": {
    "line_height": "comfortable",
  },
  "theme": {
    "dark": "One Dark",
    "light": "One Light",
    "mode": "system",
  },
  "wrap_guides": [72, 79],
}
